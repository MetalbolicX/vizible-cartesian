{
  "name": "vizible-cartesian",
  "version": "2.0.0",
  "description": "A collection of reusable charts that can be used to visualize data in a cartesian coordinate system.",
  "homepage": "https://metalbolicx.github.io/vizible-cartesian/",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "keywords": [
    "d3",
    "chart",
    "visualization",
    "typescript"
  ],
  "scripts": {
    "docs": "docsify serve docs",
    "dev": "vite",
    "build": "tsdown"
  },
  "author": {
    "name": "José Martínez Santana"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetalbolicX/vizible-cartesian.git"
  },
  "bugs": {
    "url": "https://github.com/MetalbolicX/vizible/issues"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "tipviz": "^2.0.2",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "d3": "^7.9.0"
  },
  "peerDependencies": {
    "d3": "^7.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
